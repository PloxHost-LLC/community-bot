{
  "name": "community-bot",
  "version": "1.0.0",
  "description": "An open-sourced community projected created for our discord server.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint -c .eslintrc.json --ext .ts ./src --fix",
    "precommit": "npm run lint",
    "build": "tsc -p .",
    "start": "NODE_ENV=production node ./dist/index.js",
    "prod": "npm run build && NODE_ENV=production node ./dist/index.js",
    "copy-files": "copyfiles -u 1 src/db/tables/*.sql dist/",
    "dev": "npm run build && npm run copy-files && node ./dist/index.js",
    "rc": "npx ts-node ./src/deploy-commands.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PloxHost-LLC/community-bot.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PloxHost-LLC/community-bot/issues"
  },
  "homepage": "https://github.com/PloxHost-LLC/community-bot#readme",
  "devDependencies": {
    "@types/cheerio": "^0.22.30",
    "@types/pg": "^8.6.3",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-lvksh": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-unicorn": "^40.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "@lvksh/logger": "^1.5.2",
    "@types/mysql": "^2.15.19",
    "axios": "^0.21.3",
    "cheerio": "^1.0.0-rc.10",
    "copyfiles": "^2.4.1",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "languagedetect": "^2.0.0",
    "pg": "^8.7.1",
    "puppeteer": "^13.0.1",
    "redis": "^4.0.1",
    "safe-eval": "^0.4.1",
    "web-whois": "^0.0.6",
    "whois": "^2.13.7"
  }
