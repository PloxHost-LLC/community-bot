{
  "name": "community-bot",
  "version": "1.0.0",
  "description": "An open-sourced community projected created for our discord server.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "precommit": "npm run lint",
    "build": "tsc -p .",
    "start": "NODE_ENV=production node ./dist/index.js",
    "prod": "npm run build && NODE_ENV=production node ./dist/index.js",
    "copy-files": "copyfiles -u 1 src/db/tables/*.sql dist/",
    "dev": "npm run build && npm run copy-files && node ./dist/index.js",
    "rc": "npx ts-node ./src/deploy-commands.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PloxHost-LLC/community-bot.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PloxHost-LLC/community-bot/issues"
  },
  "homepage": "https://github.com/PloxHost-LLC/community-bot#readme",
  "devDependencies": {
    "@types/pg": "^8.6.3",
    "@types/redis": "^4.0.11",
    "eslint": "^7.32.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "@lvksh/logger": "^1.5.2",
    "@types/mysql": "^2.15.19",
    "axios": "^0.21.3",
    "copyfiles": "^2.4.1",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "pg": "^8.7.1",
    "redis": "^4.0.1",
    "safe-eval": "^0.4.1",
    "whois": "^2.13.7"
  }
}
